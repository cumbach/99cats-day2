<h1>Sign Up</h1>

<form action="<%= user_url %>" method="post">
  <input
    type="hidden"
    name="authenticity_token"
    value="<%= form_authenticity_token %>">

  <input type="text" name="user[user_name]">
  <input type="text" name="user[password]">

  <input type="submit" name="sign up">
</form>
